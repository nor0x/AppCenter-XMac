<?xml version="1.0"?>
<package>
  <metadata>
    <id>nor0x.AppCenter.Analytics</id>
    <title>App Center Analytics</title>
    <authors>nor0x + Microsoft</authors>
    <owners>microsoft,appcenter</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <summary>Microsoft App Center Analytics package provides analytics capabilities for your mobile applications.</summary>
    <description>This package contains functionalities to collect session, device properties, events etc… for your application.</description>
    <copyright>© Microsoft Corporation. All rights reserved.</copyright>
    <tags>app center appcenter analytics mobilecenter xamarin.mac</tags>
    <language>en-US</language>
   <projectUrl>https://github.com/nor0x/AppCenter-XMac</projectUrl>
    <repository type="git" url="https://github.com/nor0x/AppCenter-XMac" />
    <iconUrl>https://raw.githubusercontent.com/nor0x/AppCenter-XMac/develop/art/Logo.png</iconUrl>
    <license type="expression">MIT</license>
    <dependencies>
      <dependency id="nor0x.AppCenter" version="$version$" />
    </dependencies>
    <version>$version$</version>
  </metadata>
  <files>

    <!-- .NET Standard -->
    <file src="$netstandard_dir$/Microsoft.AppCenter.Analytics.dll" target="lib/netstandard1.0" />
    <file src="$netstandard_dir$/Microsoft.AppCenter.Analytics.xml" target="lib/netstandard1.0" />

    <!-- Android -->
    <file src="$android_dir$/Microsoft.AppCenter.Analytics.dll" target="lib/MonoAndroid403" />
    <file src="$android_dir$/Microsoft.AppCenter.Analytics.xml" target="lib/MonoAndroid403" />
    <file src="$android_dir$/Microsoft.AppCenter.Analytics.Android.Bindings.dll" target="lib/MonoAndroid403" />

    <!-- iOS -->
    <file src="$ios_dir$/Microsoft.AppCenter.Analytics.dll" target="lib/Xamarin.iOS10" />
    <file src="$ios_dir$/Microsoft.AppCenter.Analytics.xml" target="lib/Xamarin.iOS10" />
    <file src="$ios_dir$/Microsoft.AppCenter.Analytics.iOS.Bindings.dll" target="lib/Xamarin.iOS10" />

    <!-- macOS -->
    <file src="$macos_dir$/nor0x.AppCenter.Analytics.dll" target="lib/Xamarin.Mac" />
    <file src="$macos_dir$/nor0x.AppCenter.Analytics.xml" target="lib/Xamarin.Mac" />
    <file src="$macos_dir$/nor0x.AppCenter.Analytics.MacOS.Bindings.dll" target="lib/Xamarin.Mac" />

    <!-- UWP -->
    <file src="$uwp_dir$/Microsoft.AppCenter.Analytics.dll" target="lib/uap10.0" />
    <file src="$uwp_dir$/Microsoft.AppCenter.Analytics.xml" target="lib/uap10.0" />

    <!-- WindowsDesktop (WPF and WinForms) for .NET Framework -->
    <file src="$windows_desktop_dir$/Microsoft.AppCenter.Analytics.dll" target="lib/net461" />
    <file src="$windows_desktop_dir$/Microsoft.AppCenter.Analytics.xml" target="lib/net461" />

    <!-- WindowsDesktop (WPF and WinForms) for .NET Core -->
    <file src="$windows_desktop_netcore_dir$/Microsoft.AppCenter.Analytics.dll" target="lib/netcoreapp3.0" />
    <file src="$windows_desktop_netcore_dir$/Microsoft.AppCenter.Analytics.xml" target="lib/netcoreapp3.0" />
  </files>
</package>
